\normalsize

\chapter{2008} 
\indent \par 

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\section{1ª Prova-14 de março de 2008}
\indent \par

\begin{enumerate}
\item Encontre uma equação da reta tangente ao gráfico da função $f(x)=\dfrac{1}{x^{2}}$ no ponto $(-2,1/4)$.
\item Calcule os seguinte limites:
\begin{enumerate}
 \item[(a)] $\displaystyle\lim_{x\rightarrow0}\dfrac{\mbox{tg}(x-\pi)}{\mbox{sen}x}$;
  \vspace{0.2cm}
 \item[(b)] $\displaystyle\lim_{x\rightarrow\infty}\dfrac{(x^{3}+2x)^{2}}{(2x-1)^{3}\cdot(x^{3}-1)}$;
\vspace{0.2cm}
 \item[(c)] $\displaystyle\lim_{x\rightarrow\frac{\pi}{2}}\dfrac{\mbox{tg} x-\sec x+\cos x}{\cos x}$.
\end{enumerate}
\item Sejam $f$ e $g$ funções contínuas em $\left[-2;5\right]$ tais que $\displaystyle\lim_{x\rightarrow-2^{+}}f(x)=7$, $\displaystyle\lim_{x\rightarrow-2^{+}}g(x)=2$, $\displaystyle\lim_{x\rightarrow5^{-}}f(x)=4$ e $\displaystyle\lim_{x\rightarrow5^{-}}g(x)=5$.
\begin{enumerate}
 \item[(a)] Calcule $\displaystyle\lim_{x\rightarrow5^{-}}\dfrac{g^{2}(x)-25}{g(x)-5}$;
  \vspace{0.2cm}
 \item[(b)] Calcule $(g-f)(-2)$ e $(g-f)(5)$;
\vspace{0.2cm}
 \item[(c)] Use o item (b) e o Teorema do Valor Intermediário para provar que existe $c\in\mathbb R$ tal que $f(c)=g(c)$.
\end{enumerate}
\item Ache um número real $b$ de modo que a função 
$$f(x)=
\left\{
\begin{array}{ccccccc}
\vspace{0.2cm}
-\dfrac{3}{2}x+5, &  & x\leq2,\\
-x^{2}+bx+(1-b), &  & x>2\\
\end{array}
\right.
$$\\
seja contínua em $x=2$. Para este valor de $b$, $f$ é contínua em $\mathbb R$? Justifique.
\item Determine as assíntotas horizontais e verticais da função $f(x)=\dfrac{\sqrt{x}+1}{\sqrt{x}-1}$.

\end{enumerate}




%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\section{2ª VPA-12 de abril de 2008}
\indent \par

\begin{enumerate}
\item Seja $f(x)=1+\mbox{sen}x+\dfrac{\mbox{sen}^{2}x}{2}+\dfrac{\mbox{sen}^{3}x}{3}+\ldots+\dfrac{\mbox{sen}^{10}x}{10}$.
\begin{enumerate}
 \item[(a)] Calcule $f'(x)$.
  \vspace{0.2cm}
  \item[(b)] Ache $f'(\frac{\pi}{6})$.
  \vspace{0.2cm}
  \item[(c)] Existe algum $x\in(0,\frac{\pi}{2})$ tal que $f'(x)=\cos x$?
\end{enumerate}
\item Seja
$$f(x)=
\left\{
\begin{array}{ccccccc}
\vspace{0.2cm}
3x^{2}, &  & x\leq1,\\
ax+b, &  & x>1.\\
\end{array}
\right.
$$\\
\begin{enumerate}
 \item[(a)] Encontre uma relação entre $a$ e $b$ de modo que $f$ seja contínua em $x=1$.
  \vspace{0.2cm}
  \item[(b)] Use a relação encontrada no item (a) para calcular $a$ e $b$, de modo que $f$ seja diferenciável em $x=1$.

[\footnotesize Sugestão: Calcule as derivadas laterais de $f$ em $x=1$.]
\end{enumerate}
\item \begin{enumerate}
 \item[(a)] Ache $y'$ onde $x^{2}-xy+\frac{3}{2}y^{2}=\dfrac{15}{2}$.
  \vspace{0.2cm}
  \item[(b)] Onde a reta normal à curva dada no ponto $(-2,1)$ intercepta a curva uma segunda vez?
\end{enumerate}
\item \begin{enumerate}
 \item[(a)] Obtenha uma equação da reta tangente ao gráfico de $f(x)=10^{x^{2}+x+1}$ no ponto $(0,10)$.
  \vspace{0.2cm}
  \item[(b)] Se $H(x)=e^{[f(x)]^{2}}$ e $f(1)=f'(1)=1$, determine uma equação da reta tangente ao gráfico de $H$ no ponto correspondente a $x=1$.
\end{enumerate}
\item \begin{enumerate}
 \item[(a)] Obtenha uma equação da reta tangente ao gráfico de $f(x)=\mbox{cossec} x+\mbox{cotg} x$ no ponto $x=\frac{\pi}{4}$.
  \vspace{0.2cm}
  \item[(b)] Determine a derivada da função $g(x)=\mbox{sen}^{2}(\cos(x^{2}+1))$.
\end{enumerate}

\end{enumerate}









%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\section{3ª Avaliação-17 de maio de 2008}
\indent \par

\begin{enumerate}
\item \begin{enumerate}
 \item[(a)] Use diferenciação logarítmica para achar a derivada da função $f(x)=x^{\ln x}$.
  \vspace{0.2cm}
  \item[(b)] Determine a abscissa do ponto no qual a reta tangente à curva $y=\cosh x$ faz com o eixo-$x$ um ângulo de $\frac{\pi}{4}$ radianos.
\end{enumerate}
\item Um homem com 1,8 $m$ de altura caminha em direção a um edifício, com velocidade de 1,5 $m/s$. Se existe um ponto de luz no chão a 15 $m$ do edifício, com que velocidade a sombra do homem no edifício estará diminuindo, quando ele estiver a 9 $m$ do edifício?
\item \begin{enumerate}
 \item[(a)] Use diferenciais para estimar (calcular aproximadamente) o número $\sqrt[3]{8,01}$.
  \vspace{0.2cm}
  \item[(b)] Seja $g$ derivável em toda a reta $\mathbb R$, crescente em $(0,+\infty)$ e decrescente em $(-\infty,0)$. Determine onde $f(x)=g(x^{2})$ é crescente e onde é decrescente.
\end{enumerate}
\item Prove que a função $f(x)=e^{\frac{x^{4}}{4}-6x^{2}+ax}$ tem, no máximo, um ponto crítico no intervalo $(-1,1)$, para qualquer valor de $a$.
\item \begin{enumerate} \item[(a)] Dada a função $f(x)=-\dfrac{x}{(x-2)^{2}}$, determine:
\begin{enumerate}
 \item[(1)] O domínio de $f$;
  \vspace{0.2cm}
  \item[(2)] As intersecções com os eixos;
  \vspace{0.2cm}
  \item[(3)] As assíntotas, caso existam;
  \vspace{0.2cm}
  \item[(4)] Os intervalos de crescimento e decrescimento;
  \vspace{0.2cm}
  \item[(5)] Os pontos de inflexão e as concavidades;
\end{enumerate}
\item[(b)] Trace o gráfico de $f$.
\end{enumerate}
\end{enumerate}




%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\section{4ª Prova-14 de junho de 2008}
\indent \par

\begin{enumerate}
\item Calcule os seguintes limites:
\begin{enumerate}
 \item[(a)] $\displaystyle\lim_{x\rightarrow+\infty}\left[\dfrac{x}{x+1}\right]^{x}$;
  \vspace{0.2cm}
  \item[(b)] $\displaystyle\lim_{x\rightarrow1}\left[\dfrac{x}{x-1}-\dfrac{1}{\ln x}\right]$.
\end{enumerate}
\item Sabendo-se que $f'(x)=3x^{2}$ e que a reta $y=3x$ é tangente ao gráfico de $f$ em algum ponto, encontre $f(x)$. \footnotesize Observação: Você poderá encontrar mais de uma função com essas características.
\normalsize
\item Determine os pontos $P$ da circunferência $x^{2}+y^{2}=25$ tais que a soma das distâncias de $P$ aos pontos $A=(-2,0)$ e $B=(2,0)$ seja
\begin{enumerate}
 \item[(a)] máxima;
  \vspace{0.2cm}
  \item[(b)] mínima.
\end{enumerate}
\item \begin{enumerate}
 \item[(a)] Calcule a área da região delimitada pelo gráfico da função $f(x)=x-2$, pelo eixo-$x$ e pelas retas $x=1$ e $x=5$. Não precisa usar somas de Riemann.
  \vspace{0.2cm}
  \item[(b)] Calcule a integral $\int_{1}^{5}(x-2)dx$.
  \item[(c)] Usando Somas de Riemann, calcule $\int_{0}^{2}(x-2)dx$.
\end{enumerate}
\item Dada a função $f(x)=\dfrac{x^{3}}{1-x^{2}}$, sabe-se que $f'(x)=\dfrac{x^{2}(3-x^{2})}{(1-x^{2})^{2}}$ e que $f''(x)=\dfrac{2x(x^{2}+3)}{(1-x^{2})^{3}}$. Indique os intervalos de crescimento e decrescimento, os pontos críticos de máximo e mínimo, os intervalos onde a concavidade é voltada para cima e os em que ela é voltada para baixo, bem como os pontos de inflexão e as assíntotas, caso existam. Feito isso, trace o gráfico.

\end{enumerate}






