\section{2ª Avaliação-15 de abril de 2011}
\indent \par

\begin{enumerate}

\item \begin{enumerate} 
\item A reta $y=x$ é tangente à parábola $y=x^2+mx+r$ no ponto de abscissa 1. Determine $m$ e $r$.
\item Se $F(x)=f(e^{x^2})$ e $G(x)=e^{(f(x))^2}$. Determine $F'(x)$ e $G'(x)$ em função de $f(x)$ e $f'(x)$.
\end{enumerate}

\item Onde a reta normal à elipse $x^2-xy+y^2=3$ no ponto $(-1,1)$ intercepta a elipse uma segunda vez?

\item 
\begin{enumerate} \item Uma maneira de definir $\sec^{-1}x$ é dizer que
$$y=\sec^{-1}x \Longleftrightarrow \sec y=x\ \ \ \text{e}\ \ \ 0\leq y\leq \frac{\pi}{2}\ \ \ \text{ou}\ \ \ \pi\leq y\leq \frac{3\pi}{2}.$$
Mostre que se essa definição for adotada, então
$$\frac{d}{dx}(\sec^{-1}x)=\frac{1}{x\sqrt{x^2-1}}.$$
\item Determine a derivada da função $f(x)=\sec^{-1}(x^2)$.
\end{enumerate}

\item 
\begin{enumerate} \item Mostre que a curva $f(x)=\tg(e^{2^x})$ não possui tangente horizontal em nenhum ponto.
\item Calcule $\displaystyle\lim_{x\rightarrow 0}\frac{\sen(x^5)}{x^2}$.
\end{enumerate}

\item Derive as seguintes funções:
\begin{enumerate}
\item $g(x)=(x^3-\sqrt{x^2+1})\sen^{-1}x$;
\item $h(x)=\displaystyle\frac{\sen x}{3^x+x}$.
\end{enumerate}

\end{enumerate}

