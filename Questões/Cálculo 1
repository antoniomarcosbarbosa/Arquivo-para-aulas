%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\section{Reavaliação AB1-17 de dezembro de 2010}
\indent \par

\begin{enumerate}

\item Sem usar a Regra de L'Hôspital, calcule os seguintes limites:
\begin{enumerate}
\item[(a)] $\displaystyle\lim_{x\rightarrow1}\dfrac{\dfrac{1}{\sqrt{x}}-1}{1-x}$;
\vspace{0.2cm}
\item[(b)] $\displaystyle\lim_{x\rightarrow0}\dfrac{\sqrt[3]{5+x}-\sqrt[3]{5}}{x}$.
\end{enumerate}

\item Determine $a$ e $b$  de tal modo que $f$ seja contínua em todos os reais, onde
$$f(x)=
\left\{
\begin{array}{cclllll}
\vspace{0.2cm}
3x, &  & \mbox{se} \ \ x\leq2,\\
\vspace{0.2cm}
ax+b, &  & \mbox{se} \ \ 2<x<5,\\
-6x, &  & \mbox{se} \ \ x\geq5.\\
\end{array}
\right.
$$\\

\item \begin{enumerate}
\item[(a)] Se $f(x)=
\left\{
\begin{array}{cclllll}
\vspace{0.2cm}
x^{2}, &  & \mbox{se} \ \ x\leq1\\
\vspace{0.2cm}
2x-1, &  & \mbox{se} \ \ x>1,\\
\end{array}
\right.
$
ache $f'(x)$ e $f''(x)$.
\vspace{0.2cm}
\item[(b)] Sejam $f(x)=e^{\mbox{tg}(g(x))}$, $g(0)=0$ e $g'(0)=3$. Determine $f'(0)$.
\end{enumerate}

\item \begin{enumerate}
\item[(a)] Se $g(x)=f^{-1}(x)$, nós sabemos que $g'(f(x))=\dfrac{1}{f'(x)}$. Lembra-se? Então, sendo $f(x)=x^{5}+3x^{3}-1$, calcule $g'(-5)$. Se esta informação puder lhe ajudar, use-a: -1 é raiz da equação $x^{5}+3x^{3}+4=0$.
\vspace{0.2cm}
\item[(b)] Sendo $f(x)=(\sec x)^{\frac{\cos x^{2}-1}{x^{3}+1}}$, determine $f'(0)$.
\end{enumerate}

\item Determine as assíntotas horizontais do gráfico de $f(x)=\dfrac{x-3}{\sqrt{x^{2}-x-6}}$.

\end{enumerate}





